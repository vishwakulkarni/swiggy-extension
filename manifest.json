{
  "manifest_version": 2,

  "name": "Swiggy Extension",
  "description": "Swiggy Extension keeps you updated about your food order ,suggests you food to order and understands you.",
  "version": "1.0",

  "icons":  {
      "16"  : "icon.png",
      "48"  : "icon.png",
      "128" : "icon.png"
      },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "popup.js"
    ]
  },
  "permissions": [
    "http://www.Swiggy.com/", "https://www.Swiggy.com/",
    "cookies",
    "geolocation",
    "sessions",
    "notifications"
  ]
}